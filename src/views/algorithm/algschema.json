{
  "title":"算法配置",
  "type":"object",
  "properties":{
    "func_zname":{
      "type":"string",
      "title":"中文ID",
      "description":"",
      "default":""
    },
    "func_name":{
      "type":"string",
      "title":"英文ID"
    },
    "func_description":{
      "type":"string",
      "format": "textarea",
      "title":"算法描述"
    },
    "n_input":{
      "type":"integer",
      "title":"INPUT个数"
    },
    "n_output":{
      "type":"integer",
      "title":"OUTPUT个数"
    },
    "del_type":{
      "type": "integer",
      "title":"预测实验删除选项设置(0不可删1可删除2建议删除)",
      "value": 0
    },
    "func_args":{
      "type":"array",
      "format":"table",
      "title":"算法参数列表",
      "uniqueItems":true,
      "items":{
        "type":"object",
        "title":"参数",
        "properties":{
          "arg_name":{
            "type":"string",
            "title":"参数英文ID"
          },
          "arg_zname":{
            "type":"string",
            "title":"参数中文ID"
          },
          "arg_type":{
            "type":"string",
            "title":"参数类型",
            "enum":[
              "columns",
              "lists",
              "list",
              "bool",
              "boollist",
              "value",
              "sql_textarea",
              "filter",
              "boolsinput",
              "inputlist",
              "addComp",
              "label_rules",
              "func_rules",
              "piemapping_rules",
              "value_recommend",
              "addThreshold",
              "column_props"
            ],
            "default":"string"
          },
          "arg_value":{
            "title":"参数值",
            "type":"string"
          },
          "arg_range":{
            "type":"string",
            "title":"参数范围"
          },
          "arg_index":{
            "type":"string",
            "title":"接收数据集下标"
          },
          "arg_relation":{
            "type":"string",
            "title":"和其他参数的关系(开始下标0)"
          },
          "arg_doc":{
            "type":"string",
            "title":"参数说明文档"
          },
          "arg_regexp":{
            "type":"string",
            "title":"验证正则(只在value组件中使用)"
          },
          "arg_nohave":{
            "type":"string",
            "title":"是否必填(1必须/空不必)"
          },
          "arg_noshow":{
            "type":"integer",
            "title":"首次加载是否显示(默认0或空显示/1不显示)"
          }
        }
      }
    },
    "python_editor":{
      "type":"string",
      "title":"Python执行代码块",
      "format":"python"
    },
    "module_name":{
      "type":"string",
      "title":"模块名称",
      "default":""
    }
  }
}
